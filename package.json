{
  "name": "cdv-doc-center",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "copy-assets": "mkdir -p ./public/lib/webviewer && cp -a ./node_modules/@pdftron/webviewer/public/* ./public/lib/webviewer && mkdir -p ./public && cp -a ./node_modules/pdfjs-dist/build/pdf.worker.min.mjs ./public/pdf.worker.min.js",
    "dev": "npm run copy-assets && next dev",
    "build": "npm run copy-assets && pnpm prisma generate && rm -rf .next && pnpm next build",
    "start": "next start",
    "lint": "next lint",
    "repo:map": "bash scripts/gen-repo-map.sh",
    "repo:make-bundle": "bash scripts/make-bundle.sh"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@pdftron/webviewer": "^11.6.1",
    "@prisma/client": "^6.13.0",
    "@tinymce/tinymce-react": "^6.3.0",
    "ali-oss": "^6.23.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.0",
    "cookies-next": "^6.1.0",
    "core-js": "^3.44.0",
    "country-flag-icons": "^1.5.19",
    "encoding": "^0.1.13",
    "exceljs": "^4.4.0",
    "formidable": "^3.5.4",
    "html-docx-js": "^0.3.1",
    "html-to-docx": "^1.8.0",
    "html2docx": "^0.1.3",
    "html2pdf.js": "^0.10.3",
    "jose": "^6.0.12",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "libreoffice-convert": "^1.6.1",
    "mammoth": "^1.10.0",
    "next": "15.4.2",
    "next-auth": "^4.24.11",
    "pdfjs-dist": "^4.8.69",
    "pdfmake": "^0.2.20",
    "playwright-chromium": "^1.54.2",
    "playwright-core": "^1.54.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^11.1.0",
    "which": "^5.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/ali-oss": "^6.16.11",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/html2pdf.js": "^0.10.0",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pdfmake": "^0.2.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
